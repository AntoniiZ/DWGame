{
  "name": "DWGame",
  "version": "1.0.0",
  "description": "Diploma project game",
  "main": "index.html",
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf dist/*",
    "html": "copyfiles -u 1 src/client/*.html dist/",
    "css": "copyfiles -u 1 src/client/*.css dist/",
    "assetsPNG": "copyfiles -u 1 src/assets/*.png dist/",
    "parcel": "parcel build src/client/game.ts -d dist/client",
    "build": "npm-run-all clean assetsPNG html css tsc parcel",
    "start": "node dist/server/server.js",
    "dev": "nodemon --watch src -e ts --exec npm-run-all build start"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/parcel-bundler": "^1.10.2",
    "@types/socket.io": "^2.1.2",
    "@types/bcrypt": "^3.0.0",
    "@types/express-session": "^1.17.0",
    "@types/mysql": "^2.15.9",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io-client": "^1.4.32",
    "@types/uniqid": "^4.1.3",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "ejs": "^3.0.1",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "phaser": "github:photonstorm/phaser#master",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.3.0",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "copyfiles": "^2.1.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "phaser3-docs": "github:photonstorm/phaser3-docs",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3333"
  }
}
